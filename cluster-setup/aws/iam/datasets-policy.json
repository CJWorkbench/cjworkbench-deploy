{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [ "s3:ListBucket", "s3:GetBucketLocation" ],
      "Resource": [
        "arn:aws:s3:::datasets.*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [ "s3:GetObject" ],
      "Resource": [
        "arn:aws:s3:::datasets.*/*"
      ]
    }
  ]
}
